{
  "name": "@2bttns/2bttns-cli",
  "version": "0.0.1-alpha",
  "description": "Create your 2bttns app with a single command.",
  "repository": {
    "type": "git",
    "url": "https://github.com/2bttns/2bttns"
  },
  "bin": {
    "@2bttns/2bttns-cli": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "prebuild": "./prebuild.sh",
    "build": "ncc build src/index.ts -o dist",
    "postbuild": "./postbuild.sh",
    "link": "npm link",
    "unlink": "npm unlink @2bttns/cli && rm -rf \"$(npm root -g)/@2bttns/cli\""
  },
  "keywords": [
    "2bttns"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.3.3",
    "@types/tmp": "^0.2.3",
    "@vercel/ncc": "^0.36.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.2.10",
    "tmp": "^0.2.1",
    "zod": "^3.21.4"
  }
}