{
  "name": "@2bttns/cli",
  "version": "0.0.1-alpha",
  "description": "Create your 2bttns app with a single command.",
  "repository": {
    "type": "git",
    "url": "https://github.com/2bttns/2bttns"
  },
  "bin": {
    "@2bttns/cli": "./dist/index.js"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx tsx src/index.ts",
    "prebuild": "npm run unlink && rm -rf dist",
    "build": "ncc build src/index.ts -o dist",
    "postbuild": "npm run link",
    "link": "npm link",
    "unlink": "npm unlink @2bttns/cli && rm -rf \"$(npm root -g)/@2bttns/cli\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "2bttns"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/got": "^9.6.12",
    "@types/node": "^20.3.3",
    "@types/tar": "^6.1.5",
    "@vercel/ncc": "^0.36.1",
    "got": "^13.0.0",
    "tar": "^6.1.15",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "commander": "^11.0.0"
  }
}